.listing .block {
	padding: 15px;
    background-color: #fff;
    margin-bottom: 20px;
	width: 100%;
    float: left;
	position:relative;
	box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
