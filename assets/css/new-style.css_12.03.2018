.listing .block .itm {
	position:relative;
	margin:10px 0;
}
.listing .block .hover_itm {
	background-color:rgba(43, 181, 244,0.8);
	position:absolute;
	left:0;
	top:0;
	width:100%;
	height:100%;
	display:none;
	transition: all 0.3s ease-in-out;
